{
	"node"     : true,
	"bitwise"  : true,
	"browser"  : true,
	"camelcase": true,
	"curly"    : true,
	"devel"    : true,
	"eqeqeq"   : true,
	"esnext"   : true,
	"forin"    : true,
	"freeze"   : true,
	"globals"  : {
		"angular": false,
		"$"      : false
	},
	"immed"    : true,
	"indent"   : 4,
	"latedef"  : true,
	"newcap"   : true,
	"noarg"    : true,
	"noempty"  : true,
	"nonew"    : true,
	"quotmark" : "double",
	"regexp"   : true,
	"smarttabs": true,
	"strict"   : true,
	"trailing" : true,
	"undef"    : true,
	"unused"   : "vars"
}